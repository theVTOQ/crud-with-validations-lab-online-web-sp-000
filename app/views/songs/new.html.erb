<h1>Create Song</h1>
<%= form_for @song, method: "post" do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :release_year %>
  <%= f.check_box :released %>
  <!--
  <%= f.radio_button :released, true %>
  <%= f.radio_button :released, false %>
  -->
  <%= f.text_field :genre %>
  <%= f.text_field :artist_name %>

  <%= f.submit "Create" %>
<% end %>
